                                    const selectedPcountry = PcountryDropdown.value;

                                    // Clear existing options
                                    PstateDropdown.innerHTML = '<option value="">Select State</option>';

                                    // Populate states based on the selected Pcountry
                                    for (const Pstate in PcountryStateCity[selectedPcountry]) {
                                        const option = document.createElement('option');
                                        option.value = Pstate;
                                        option.textContent = Pstate;
                                        PstateDropdown.appendChild(option);
                                    }

                                    // Trigger population of cities for the selected Pstate
                                    populatePcities();
                                }

                                function populatePcities() {
                                    const PstateDropdown = document.getElementById('Pstate');
                                    const PcityDropdown = document.getElementById('Pcity');
                                    const selectedPcountry = document.getElementById('Pcountry').value;
                                    const selectedPstate = PstateDropdown.value;

                                    // Clear existing options
                                    PcityDropdown.innerHTML = '<option value="">Select City</option>';

                                    // Populate cities based on the selected Pcountry and Pstate
                                    const cities = PcountryStateCity[selectedPcountry][selectedPstate];
                                    if (cities) {
                                        cities.forEach(city => {
                                            const option = document.createElement('option');
                                            option.value = city;
                                            option.textContent = city;
                                            PcityDropdown.appendChild(option);
                                        });
                                    }
                                }

                                // Initial population of states
                                populatePstates();
